---
// レイアウト読み込み
import BaseLayout from "../../components/BaseLayout.astro"
// props
const lang = "ja";
const type = "website";
const title = "About";
const description = "Indexivは大学院生によって個人的に運営されているサイトです。理論物理学や関連する数学について学んだことをまとめています。";
---
<!-- レイアウト呼び出し -->
<BaseLayout lang={lang} type={type} title={title} description={description}>
    <h1>{title}</h1>
    <p>{description}</p>
</BaseLayout>